{
  "manifest_version": 3,
  "name": "LXGW WenKai Light Font Extension",
  "version": "1.0",
  "description": "Apply LXGW WenKai Light to all web pages.",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
